<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Chame</title>

    <link rel="stylesheet" href="/css/bootstrap.css" />
    <link rel="stylesheet" href="/css/fontawesome.css" />
    <link rel="stylesheet" href="/css/site.css" />

    <link rel="stylesheet" href="/chame-loader/css?filter=customization.css" />
</head>
<body>
    <main>
        <div class="container">
            <div class="jumbotron page">
                <h1>
                    Demo
                </h1>
                <h2>
                    This simple demo shows you how Chame works.
                </h2>

                <div class="demo-container">
                    @Html.Partial("DemoPartialView")
                </div>

                <form asp-controller="Demo" asp-action="SwitchTheme" method="post">
                    <button type="submit" class="btn btn-primary btn-lg">Click here to switch theme!</button>
                </form>
            </div>
        </div>
    </main>
    <script src="/js/jquery.js"></script>
    <script src="/js/bootstrap.js"></script>
    <script src="/js/site.js"></script>

    <script src="/chame-loader/js?filter=customization.js"></script>
</body>
</html>



